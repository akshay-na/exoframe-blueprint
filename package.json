{
  "name": "express-js-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate to start a node project",
  "email": "akshayna04@gmail.com",
  "author": "akshay-na",
  "license": "MIT",
  "homepage": "https://github.com/akshay-na/express-js-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akshay-na/express-js-template.git"
  },
  "bugs": {
    "url": "https://github.com/akshay-na/express-js-template/issues"
  },
  "scripts": {
    "dist": "gulp build",
    "commit": "cz",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky",
    "pre-commit": "npx lint-staged",
    "pre-push": "npm run test:cov",
    "release": "HUSKY_SKIP_HOOKS=1 CI=true npx -y semantic-release --no-verify",
    "start": "env NODE_ENV=development ts-node-dev --respawn --transpile-only --require tsconfig-paths/register src/app/servers/BoilerplateServer.ts",
    "start:prod": "node dist/app/servers/BoilerplateServer.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "lint-staged": {
    "**/*.ts": ["eslint --fix"],
    "*.{js,jsx,ts,tsx,json,css,scss,md,yaml,yml}": [
      "npx prettier --list-different"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "bcrypt": "5.1.1",
    "body-parser": "1.20.3",
    "compression": "1.8.0",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.21.1",
    "express-brute": "1.0.1",
    "express-fileupload": "1.5.1",
    "express-flash": "0.0.2",
    "express-rate-limit": "7.5.0",
    "express-winston": "4.2.0",
    "helmet": "8.0.0",
    "passport": "0.7.0",
    "reflect-metadata": "0.2.2",
    "uuid": "11.0.5",
    "winston": "3.17.0",
    "winston-daily-rotate-file": "5.0.0",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "6.6.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "commitizen": "^4.3.1",
    "conventional-commit-types": "^3.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "del": "8.0.0",
    "depcheck": "1.4.7",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "50.5.0",
    "eslint-plugin-prettier": "^5.2.1",
    "gulp": "5.0.0",
    "gulp-cached": "1.1.1",
    "gulp-notify": "5.0.0",
    "gulp-plumber": "1.2.1",
    "gulp-remember": "1.0.1",
    "gulp-typescript": "6.0.0-alpha.1",
    "gulp-uglify": "3.0.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-junit": "16.0.0",
    "jest-pnp-resolver": "1.2.3",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "semantic-release": "^24.2.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  }
}
